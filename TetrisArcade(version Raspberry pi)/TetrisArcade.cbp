				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="sfml-audio" />
					<Add library="sfml-graphics" />
					<Add library="sfml-system" />
					<Add library="sfml-window" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-g" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="main.cpp">
			<Option virtualFolder="Src/" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
