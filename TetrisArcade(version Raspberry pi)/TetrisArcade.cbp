<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="TetrisArcade" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="Hearders/;Sources/;Src/;" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/TetrisArcade" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="sfml-audio" />
					<Add library="sfml-graphics" />
					<Add library="sfml-system" />
					<Add library="sfml-window" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/TetrisArcade" prefix_auto="1" extension_auto="1" />
